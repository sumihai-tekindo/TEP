<odoo>
    <data>
        <record id="view_inventory_form_inherit_totalindo" model="ir.ui.view">
            <field name="name">stock.inventory.form.inherit.totalindo</field>
            <field name="model">stock.inventory</field>
            <field name="inherit_id" ref="stock.view_inventory_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='accounting_date']" position="after">
                    <field name="type_barang"/>
                    <field name="oleh"/>
                    <field name="department_id"/>
                    <field name="referensi"/>
                </xpath>
            </field>
        </record>    	
    </data>
</odoo>